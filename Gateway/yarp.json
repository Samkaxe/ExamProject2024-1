{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Clusters": {
      "Authentication": {
        "Destinations": {
          "Auth_0": {
            "Address": "http://authservice:80"
          }
        }
      }
    },
    "Routes": {
      "register": {
        "ClusterId": "Authentication",
        "Match": {
          "Path": "/Auth/register"
        }
      }
    }
  }
}
